<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="2">
    <title>SML/NJ Version VERSION</title>
    <background file="background.jpg" mime-type="image/jpeg" scaling="tofit" alignment="center"/>
    <options customize="never" require-scripts="false"/>
    <welcome file="welcome.html" mime-type="text/html" />
    <readme file="readme.html" mime-type="text/html" />
    <license file="license.html" mime-type="text/html" />
    <conclusion file="conclusion.html" mime-type="text/html" />
    <os-version min="10.6" />
    <options customize="never" require-scripts="false"/>
    <choice id="choice8" customLocation="/usr/local/smlnj" customLocationAllowAlternateVolumes="true" visible="false">
	<pkg-ref id="org.smlnj.x86.pkg" version="VERSION">smlnj.pkg</pkg-ref>
    </choice>
    <choices-outline>
        <line choice="choice8" />
    </choices-outline>
</installer-gui-script>
